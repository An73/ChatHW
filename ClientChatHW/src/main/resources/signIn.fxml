<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: coral;" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="authorized.SignIn">
    <children>
        <TextArea fx:id="loginBox" layoutX="50.0" layoutY="50.0" maxHeight="40.0" maxWidth="300.0" minHeight="40.0" minWidth="300.0" promptText="LOGIN" style="-fx-control-inner-background:#FFA07A;" />
        <PasswordField fx:id="passwordBox" layoutX="50.0" layoutY="120.0" maxHeight="40.0" maxWidth="300.0" minHeight="40.0" minWidth="300.0" promptText="PASSWORD" style="-fx-control-inner-background:#FFA07A;" />
        <Button fx:id="signInButton" layoutX="80.0" layoutY="190.0" maxHeight="40.0" maxWidth="240.0" minHeight="40.0" minWidth="240.0" text="SignIn" textFill="#FF8C00" />
        <Button fx:id="signUpButton" layoutX="80.0" layoutY="260.0" maxHeight="40.0" maxWidth="240.0" minHeight="40.0" minWidth="240.0" text="SignUp" textFill="#FF8C00" />
    </children>
</AnchorPane>
